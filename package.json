{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Premium lecturer portfolio website built with Angular 16",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --configuration development --open",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:analyze": "ng build --configuration production --stats-json && npx webpack-bundle-analyzer dist/portfolio/stats.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:prod": "ng build --configuration production && http-server dist/portfolio -p 4200",
    "analyze": "ng build --stats-json && npx webpack-bundle-analyzer dist/portfolio/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@angular/service-worker": "^16.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.15",
    "@angular/cli": "^16.2.15",
    "@angular/compiler-cli": "^16.2.0",
    "@angular-eslint/builder": "^16.0.0",
    "@angular-eslint/eslint-plugin": "^16.0.0",
    "@angular-eslint/eslint-plugin-template": "^16.0.0",
    "@angular-eslint/schematics": "^16.0.0",
    "@angular-eslint/template-parser": "^16.0.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.7.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "http-server": "^14.1.1",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "protractor": "~7.0.0",
    "typescript": "~5.1.3",
    "webpack-bundle-analyzer": "^4.8.0"
  },
  "keywords": [
    "angular",
    "portfolio",
    "lecturer",
    "academic",
    "website",
    "responsive",
    "modern"
  ],
  "author": "Dr. Sarah Johnson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/portfolio/issues"
  },
  "homepage": "https://your-username.github.io/portfolio",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions",
    "iOS >= 12",
    "Android >= 8"
  ]
}
